
public class Main {

	public static void main(String[] args) {
		
		/*Comando de atribuição
		  
		 	Sintaxe:
			<variável> = <expressão>;
		 */

	// Exemplo 1
		int x, y;
		x = 5;
		y = 2 * x;
		
		System.out.println(x);
		System.out.println(y);
		
	// Exemplo 2
		int x;
		double y;
		x = 5;
		y = 2 * x;
		
		System.out.println(x);
		System.out.println(y);
		
	}

}
